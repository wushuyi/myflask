{
  "name": "highlight.js",
  "filename": "highlight.min.js",
  "version": "8.7",
  "description": "Easy-to-use, Javascript-based syntax highlighter",
  "homepage": "http://highlightjs.org",
  "keywords": [
    "highlight",
    "syntax highlighter"
  ],
  "maintainers": [
    {
      "name": "Diwaker Gupta"
    },
    {
      "name": "Ivan Sagalaev",
      "email": "maniac@softwaremaniacs.org"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/isagalaev/highlight.js"
    }
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/highlightjs/cdn-release.git",
    "basePath": "build",
    "files": [
      "highlight.min.js",
      "languages/*.min.js",
      "styles/*"
    ]
  }
}
